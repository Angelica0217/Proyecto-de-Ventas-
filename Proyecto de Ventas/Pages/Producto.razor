@page "/Producto"
@using Proyecto_de_Ventas.Data

<h3>Producto</h3>
<div class="container">
    <div class="row">
        @foreach (var ventaRopaCard in DatosVR)
        {
            <div class="col-md-4 mb-4">
                <div class="card">
                    <img src="@ventaRopaCard.Foto" class="card-img-top" alt="Imagen">
                    <div class="card-body">
                        <h5 class="card-title">@ventaRopaCard.Nombre</h5>
                        <p class="card-text">@ventaRopaCard.Descripcion</p>
                        <p class="card-text">Precio: @ventaRopaCard.Precio</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>

<div class="Producto1">
@if (DatosVR.Any())
{
    foreach(var DatosVentR in DatosVR)
    {
        <Proyecto_de_Ventas.Componentes.VentasP DatosventR = "@DatosVentR"/>
    }
}
</div>
